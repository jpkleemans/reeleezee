	<xs:simpleType name="RecoveryHandlingType">
		<xs:restriction base="xs:token">
			<xs:enumeration value="Reject">
				<xs:annotation>
					<xs:documentation>Blocks a deleted object from being changed</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Reactivate">
				<xs:annotation>
					<xs:documentation>Automatically undeletes a deleted object</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Forsake">
				<xs:annotation>
					<xs:documentation>Removes the identifiers from the deleted object and creates a new one</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Update">
				<xs:annotation>
					<xs:documentation>Updates the object, but keeps it marked as deleted</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Ignore">
				<xs:annotation>
					<xs:documentation>Skips any deleted objects</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

